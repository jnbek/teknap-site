alias sayc say $cparse("$*")
alias echoc echo $cparse("$*")

alias fade
{
 switch ($0)
 {
  (-ryg)
  {sayc $clf(1 $1-)}
  (-cgw)
  {sayc $clf(11 $1-)}
  (-rwb)
  {sayc $clf(21 $1-)}
  (-gw)
  {sayc $clf(31 $1-)}
  (-pb)
  {sayc $clf(41 $1-)}
  (-yg)
  {sayc $clf(51 $1-)}
  (*) {echoc %WError: %n-ryg, -cgw, -rwb, -gw, -pb, or -yg only!}
 }
}

alias efade
{
 switch ($0)
 {
  (-ryg)
  {echoc $clf(1 $1-)}
  (-cgw)
  {echoc $clf(11 $1-)}
  (-rwb)
  {echoc $clf(21 $1-)}
  (-gw)
  {echoc $clf(31 $1-)}
  (-pb)
  {echoc $clf(41 $1-)}
  (-yg)
  {echoc $clf(51 $1-)}
  (*) {echoc %WError: %n-ryg, -cgw, -rwb, -gw, -pb, or -yg only!}
 }
}

alias clf (cnt, text)
{
 @:text_line=[]
 switch ($cnt)
 {
  (1)
  {
   fec ($text) letters
   {
    if (cnt==11) {@cnt=1}
    @text_line#=color_list($cnt)##letters
    @cnt++
   }
   return $text_line
  }
  (11)
  {
   fec ($text) letters
   {
    if (cnt==21) {@cnt=11}
    @text_line#=color_list($cnt)##letters
    @cnt++
   }
   return $text_line
  }
  (21)
  {
   fec ($text) letters
   {
    if (cnt==31) {@cnt=21}
    @text_line#=color_list($cnt)##letters
    @cnt++
   }
   return $text_line
  }
  (31)
  {
   fec ($text) letters
   {
    if (cnt==41) {@cnt=31}
    @text_line#=color_list($cnt)##letters
    @cnt++
   }
   return $text_line
  }
  (41)
  {
   fec ($text) letters
   {
    if (cnt==51) {@cnt=41}
    @text_line#=color_list($cnt)##letters
    @cnt++
   }
   return $text_line
  }
  (51)
  {
   fec ($text) letters
   {
    if (cnt==61) {@cnt=51}
    @text_line#=color_list($cnt)##letters
    @cnt++
   }
   return $text_line
  }
  (*)
  {
   return 0
  }
 }


alias color_list (num)
{
 switch ($num)
 {
