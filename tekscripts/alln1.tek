{
	eval msg $nick $reverse($text)
}
alias ksayb (nick, text)
{
	raw 610 $nick $reverse($text)
}
alias msayb (nick, text)
{
	raw 622 $nick $reverse($text)
}
alias bsayb (nick, text)
{
	raw 612 $nick $reverse($text)
}
alias esayb (text)
{
	eval me $reverse($text)
}
alias umsayb (nick, text)
{
	eval unmuzzle $nick $reverse($text)
}
