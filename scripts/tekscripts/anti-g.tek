/* I made the /diegnomes alias much smaller and more efficient -Tu */
alias diegnomes
{
 raw 628 $tr(/"//$(A$rand($ag_lines)))
 ^guser -l -u -cmd
 {
  ^whois -cmd
  {
   if (rmatch("AudioGnome* $9))
   {
    raw 610 $0 $(A$rand($ag_lines))
   }
  } $0
 }
}
